   :density 1d0
   :radius .5d0
   :height 1d0))

(defmethod free ((object physic-basket))
  (with-slots (body data geom orot vertices indices) object
    (when body (%ode:body-destroy body))
    (when geom (%ode:geom-destroy geom))
    (claw:free orot)
    (claw:free vertices)
    (claw:free indices)
    (claw:free data)))

#+nil
(setf ode-vertices (claw:alloc :float (* 3 gvl))
      ode-indices  (claw:alloc :unsigned-int gil))

(defmethod initialize-instance :after ((obj physic-basket) &key)
  (with-slots (mass body geom
               data
               buf
               pos rot
               vertices indices
               density radius height)
      obj
    (let ((gvl (length *bk-vertices*))
          (gil (length *bk-indices*)))

      (claw:c-let ((ode-vertices :float        :calloc t :count gvl)
                   (ode-indices  :unsigned-int :calloc t :count gil)
                   (mesh-data %ode::tri-mesh-data-id
                              :ptr (%ode:geom-tri-mesh-data-create))
                   (m %ode:mass :from mass))
        ;;
        (setf data     (mesh-data &)
              indices  (ode-indices &)
              vertices (ode-vertices &))
        ;;
        (loop :for index :in *bk-indices*
              :for i :from 0
              :do (setf (ode-indices i) index))
        ;;
        (loop :for vertex :in *bk-vertices*
              :for vindex :from 0
              :do (setf (ode-vertices vindex) vertex))
        ;;
        (%ode:geom-tri-mesh-data-build-single (mesh-data &)
                                              (ode-vertices &)
                                              (* 3 (claw:sizeof :float))
                                              (/ gvl 3)
                                              (ode-indices &)
                                              gil
                                              (* 3 (claw:sizeof :unsigned-int)))
        (setf geom (%ode:create-tri-mesh *space* (mesh-data &) 0 0 0))
        ;;(%ode:mass-set-trimesh (m &) density geom)
        ;;(%ode:body-set-mass body (m &))
        ;;(%ode:geom-set-body geom body)
        (%ode:geom-tri-mesh-enable-tc geom %ode:+sphere-class+ 0)
        (%ode:geom-tri-mesh-enable-tc geom %ode:+box-class+    0)
        ))
    (ode-update-pos obj pos)
    (ode-update-rot obj rot)))

(defstruct-g basket
  (position :vec3 :accessor pos))

